{
  "compilerOptions": {
    "outDir": "./build/",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "composite": true
  },
}
